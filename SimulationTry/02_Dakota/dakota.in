

method
  coliny_cobyla
  initial_delta 0.1
  scaling
  seed 12345
  output debug
  max_function_evaluations = 20
 
variables
  continuous_design = 5
  cdv_initial_point  1 1 1 1 1
  cdv_lower_bounds  0 0 0 0 0
  cdv_upper_bounds  10 2 2 2 2
  cdv_descriptor  '|Abstand' '|Ellipse1B' '|Ellipse1A' '|Ellipse2A' '|Ellipse2B'
  scale_types  'auto' 'auto' 'auto' 'auto' 'auto'

responses
  objective_functions = 0
  nonlinear_inequality_constraints = 0
  no_gradients
  no_hessians
  
interface
  fork
  analysis_driver = 'fdakota_client.exe'
  verbatim  parameters_file = 'params.in'
  results_file    = 'results.out'
  work_directory directory_tag
  named 'design' file_save directory_save

  asynchronous
  evaluation_concurrency = 8
